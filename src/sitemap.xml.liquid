---
permalink: /sitemap.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{%- for page in collections.all %}
  {%- unless page.url contains '/SkillsFramework/' %}
    {%- if page.url and page.url != '/sitemap.xml' %}
  <url>
    <loc>https://organised.team{{ page.url | url }}</loc>
    <lastmod>{{ page.date | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>{{ page.data.changefreq | default: 'monthly' }}</changefreq>
    <priority>{{ page.data.priority | default: '0.5' }}</priority>
  </url>
    {%- endif %}
  {%- endunless %}
{%- endfor %}
</urlset>